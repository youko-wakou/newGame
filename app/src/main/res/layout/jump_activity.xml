<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent" android:layout_height="match_parent" android:background="@drawable/back">
    <ImageView android:id="@+id/yari" android:layout_width="80dp" android:layout_height="200dp" android:layout_alignParentBottom="true" android:layout_alignParentStart="true" android:layout_marginStart="25dp" app:srcCompat="@drawable/yari"/>

    <ImageView
        android:id="@+id/neko"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignStart="@+id/start"
        android:layout_alignTop="@+id/yari"
        android:layout_marginTop="46dp"
        app:srcCompat="@drawable/neko" />
    <Button android:id="@+id/start" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:layout_marginTop="68dp" android:text="START"/>
</RelativeLayout>